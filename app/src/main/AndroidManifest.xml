        android:roundIcon="@mipmap/person_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.PorfolioApp"
        tools:targetApi="31">
        <activity
            android:name=".Splash"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ContactScreen"
            android:exported="false" />
        <activity
            android:name=".Navigation"
            android:exported="false" />
        <activity
            android:name=".OnBoardingScreen"
            android:exported="false"/>


    </application>

</manifest>